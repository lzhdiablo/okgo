<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzh.dao.MiddleTagDao">


	<select id="getMiddleTagByTagName" resultType="com.lzh.entity.MiddleTag">
		select m.* from middletag m where m.tagname=#{middletag}
	</select>
</mapper>